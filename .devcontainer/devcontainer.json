{
    "name": "doclayer",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "..",
		"args": {
			"VARIANT": "1.91.0-bookworm"
		}
    },
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer"
			]
		}
	},
	"postCreateCommand": "pre-commit install --install-hooks",
	"remoteUser": "dev"
}